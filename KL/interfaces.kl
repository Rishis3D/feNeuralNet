/// An interface for different types of neural network layers
interface ILayer {
  setNeuronSize!(Integer neurons);
  Integer getNeuronSize();
  setTransferFunc!(String transferFunc);
  String getTransferFunc();
  setType!(String transferFunc);
  String getType();
  setIndex!(Integer index);
  Integer getIndex();
  setParentIndex!(Integer index);
  Integer getParentIndex();
};